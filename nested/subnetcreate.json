{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"location": {
			"type": "string",
			"metadata": {
				"description": "Datacenter Region Location"
			}
		},
		"apiVersionNetwork": {
			"type": "string",
			"metadata": {
				"description": "Network API version"
			}
		},
		"existingVnetName": {
			"type": "string",
			"metadata": {
				"description": "Existing virtual network name"
			}
		},
		"subnetName": {
			"type": "string",
			"metadata": {
				"description": "Name of the subnet for Master nodes and Bastion node"
			}
		},
		"subnetPrefix": {
			"type": "string",
			"metadata": {
				"description": "Master subnet CIDR"
			}
		},
		"networkSecurityGroup": {
			"type": "string",
			"metadata": {
				"description": "Nsg name"
			}
		},
		"customTags": {
			"type": "object",
			"metadata": {
				"description": "Custom Tags"
			}
		}
	},
	"variables": {
	},
	"resources": [
		{
			"apiVersion": "[parameters('apiVersionNetwork')]",
			"type": "Microsoft.Network/virtualNetworks/subnets",
			"name": "[concat(parameters('existingVnetName'), '/', parameters('subnetName'))]",
			"location": "[parameters('location')]",
			"tags": "[parameters('customTags')]",
			"dependsOn": [
			],
			"properties": {
				"addressPrefix": "[parameters('subnetPrefix')]",
				"networkSecurityGroup": {
					"id": "[parameters('networkSecurityGroup')]"
				}
			}
		}
	],
	"outputs": {}
}